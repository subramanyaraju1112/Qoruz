{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@qoruz/*": ["./src/*"],
      "@stitches-config/*": ["./src/stitches/*"],
      "@app/*": ["./src/pages/*"],
      "@components/*": ["./src/components/*"],
      "@lib/*": ["./src/lib/*"],
      "@constant/*": ["./src/constant/*"],
      "public/*": ["./public/*"]
    },
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/marketing-tool/InfluencerFollower/hero"],
  "exclude": ["node_modules"],
  "moduleResolution": ["node_modules", ".next", "node"]
}
